parameters:
    excludes_analyse:
        - %rootDir%/../../../tests/*
        - %rootDir%/../../../vendor/*
    ignoreErrors:
        - '#Casting to .+ something that.s already .+\.#'
        - '#Argument of an invalid type DOMDocument\|false\|SimpleXMLElement supplied for foreach, only iterables are supported\.#'
        - '#Parameter \#1 \$element of method Zend_Config_Xml::_processExtends\(\) expects SimpleXMLElement, DOMDocument\|false\|SimpleXMLElement given\.#'
